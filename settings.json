{
  "icon_theme": "Warm Charmed Icons",
  "theme": "Aura Dark",
  "theme_overrides": {
    "Aura Dark": {
      "border.variant": "#15141C",
      "border": "#15141C",
      "title_bar.background": "#15141C",
      "panel.background": "#15141C",
      "panel.focused_border": "#4E466E",
      "players": [
        {
          "cursor": "#BD9DFF",
        },
      ],
      "syntax": {
        "comment": {
          "font_style": "italic",
        },
        "comment.doc": {
          "font_style": "italic",
        },
      },
    },
  },
  "title_bar": {
    "show_sign_in": false,
    "show_onboarding_banner": false,
    "show_project_items": false,
    "show_branch_name": false,
    "show_user_menu": false,
  },
  "tab_bar": {
    "show": true,
    "show_tab_bar_buttons": false,
    "show_nav_history_buttons": false,
  },
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false,
  },
  "status_bar": {
    "experimental.show": false,
  },
  "project_panel": {
    "dock": "right",
    "default_width": 400,
    "hide_root": true,
    "auto_fold_dirs": false,
    "starts_open": false,
    "git_status": false,
    "sticky_scroll": false,
    "scrollbar": {
      "show": "never",
    },
    "indent_guides": {
      "show": "never",
    },
    "hide_gitignore": true,
  },
  "outline_panel": {
    "default_width": 300,
    "indent_guides": {
      "show": "never",
    },
  },
  "file_finder": {
    "modal_max_width": "large",
  },
  "vim_mode": true,
  "relative_line_numbers": "enabled",
  "disable_ai": true,
  "scrollbar": {
    "show": "never"
  },
  "indent_guides": {
    "enabled": false,
  },
  "gutter": {
    "breakpoints": false,
    "folds": false,
    "min_line_number_digits": 0
  },
  "session": {
    "trust_all_worktrees": true
  },
  "scroll_beyond_last_line": "off",
  "show_wrap_guides": true,
  "wrap_guides": [100, 150],
  "restore_on_startup": "none",
  "experimental.theme_overrides": {
    "editor.wrap_guide": "#15141F",
    "tab.active_background": "#121117",
    "tab.inactive_background": "#15141F",
    "tab_bar.background": "#15141F"
  }
}
