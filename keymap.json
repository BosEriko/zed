//                                                                                      //
// ==================================================================================== //
[
  // ================================================================================== //
  // [ctrl+\] Open Explorer                                                             //
  // ================================================================================== //
  {
    "context": "!Editor && !Terminal && !Search",
    "bindings": {
      "ctrl-\\": "workspace::ToggleRightDock"
    }
  },
  {
    "context": "Editor || Terminal || Search",
    "bindings": {
      "ctrl-\\": "project_panel::ToggleFocus"
    }
  }
]
